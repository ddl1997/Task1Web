<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">

    <servlet>
        <servlet-name>FromDatabaseToExcelServlet</servlet-name>
        <servlet-class>servlet.FromDatabaseToExcelServlet</servlet-class>
    </servlet>
    <servlet-mapping>
           <servlet-name>FromDatabaseToExcelServlet</servlet-name>
           <url-pattern>/servlet/FromDatabaseToExcelServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>FromJsonToExcelServlet</servlet-name>
        <servlet-class>servlet.FromJsonToExcelServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>FromJsonToExcelServlet</servlet-name>
        <url-pattern>/servlet/FromJsonToExcelServlet</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>FromExcelToJsonServlet</servlet-name>
        <servlet-class>servlet.FromExcelToJsonServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>FromExcelToJsonServlet</servlet-name>
        <url-pattern>/servlet/FromExcelToJsonServlet</url-pattern>
    </servlet-mapping>
</web-app>